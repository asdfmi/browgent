{
  "name": "agent-flow-root",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "portal",
    "runner"
  ],
  "scripts": {
    "lint": "eslint .",
    "dev:portal": "pnpm --dir portal run dev:api",
    "dev:portal:ui": "pnpm --dir portal run dev:ui",
    "dev:runner": "pnpm --dir runner run dev:runner",
    "dev": "concurrently \"pnpm run dev:portal:ui\" \"pnpm run dev:portal\" \"pnpm run dev:runner\""
  },
  "imports": {
    "#domain/*": "./portal/domain/*"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "concurrently": "^9.1.0",
    "tsx": "^4.19.1"
  },
  "packageManager": "pnpm@10.20.0"
}
